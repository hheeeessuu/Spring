<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <!-- namespace -->
<mapper namespace="com.dept">
	<select id="selectAll" resultType="DeptDTO">
		select deptno, dname, loc
		from dept
		order by deptno
	</select>
</mapper>